{
  "extends": ["expo", "prettier"],
  "plugins": ["@typescript-eslint", "unused-imports", "prettier"],
  "rules": {
    "no-console": "warn",
    "@typescript-eslint/no-unused-vars": "error",
    "unused-imports/no-unused-imports": "warn"
  },
  "settings": {
    "import/resolver": {
      "typescript": {}
    }
  }
}
